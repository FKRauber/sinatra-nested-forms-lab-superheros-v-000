<div class="team">
  <h1>Name: <%= @team.name %></h1><br><br>
  <h2>Team Motto: <%= @team.motto %></h2><br><br>
</div><br>

<% @super_hero.each do |hero| %>

		  <div class="hero">
        <h2>Hero Name: <%= super_hero.name %></h2><br>
		    <p>Hero Power: <%= super_hero.type %></p><br>
		    <p>Hero Biography: <%= super_hero.booty %></p><br>

		  </div>

<% end %>
